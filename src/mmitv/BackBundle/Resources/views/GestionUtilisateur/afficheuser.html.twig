{% extends 'mmitvBackBundle:Dashboard:dashboard.html.twig' %}

{% block title %} {{ parent() }}- Gestion Utilisateurs{% endblock %}

{% block menu %}{{ parent() }}{% endblock %}

{% block body %}
    Affiche les utilisateurs sous tableau<br/><br/>

    <table>
    {% for user in users %}

        <tr>
        <td>{{ user.username }}</td>
        <td>{{ user.email }}</td>
        <td><a href="">Modifier</a></td>
        <td><a href="">Supprimer</a></td>
        </tr>

    {% endfor %}
    </table>
{% endblock %}